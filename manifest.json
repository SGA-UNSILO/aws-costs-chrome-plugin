{
  "name": "AWS Costs Plugin",
  "version": "0.1",
  "description": "Provide users with in-page information about AWS costs.",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "js": [ "jquery-3.4.1.js", "updateAWSView.js"],
      "matches": ["https://console.aws.amazon.com/*", "https://a0.p.awsstatic.com/*"],
      "run_at" : "document_end"
    }
  ],
  "manifest_version": 2
}
